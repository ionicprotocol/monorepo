{
  "name": "@midas-capital/chains",
  "repository": "git://github.com:midas-protocol/monorepo.git",
  "version": "0.3.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "description": "",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "files": [
    "/dist/"
  ],
  "scripts": {
    "prune": "node ./scripts/prune.js ./deployments/*.json",
    "build": "yarn run prune && tsc -p tsconfig-sdk.json && tsc -p tsconfig-cjs.json",
    "build:esm": "tsc -p tsconfig-sdk.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "clean": "rm -rf dist",
    "prettier:ts": "prettier --write 'src/**/*.ts'",
    "lint": "eslint . -c .eslintrc.json --ext ts,json",
    "lint-fix": "eslint . -c .eslintrc.json --fix --ext ts,json"
  },
  "keywords": [],
  "author": "Midas Capital",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.7.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@midas-capital/types": "workspace:*",
    "ethers": "^5.6.9",
    "utf-8-validate": "^5.0.9"
  }
}
