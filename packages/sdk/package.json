{
  "name": "@ionicprotocol/sdk",
  "repository": "git://github.com:ionicprotocol/monorepo.git",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "description": "",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "files": [
    "/dist/",
    "/artifacts"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "yarn run build:forge; yarn run artifacts; yarn run typechain; yarn run abimate; yarn run build:sdk",
    "postbuild": "du -sk dist/cjs; du -sk dist/esm",
    "clean": "rm -rf dist; rm -rf ./typechain; rm -rf ./artifacts; rm -rf ./abis",
    "artifacts": "echo '> Gathering artifacts...'; ./scripts/move-artifacts.sh",
    "typechain": "echo '> Creating typechain types...'; [ ! -d ./typechain ] && typechain --target ethers-v5 --out-dir './typechain' './artifacts/*.json' --show-stack-traces",
    "abimate": "echo '> Extracting ABIs from artifacts...'; abimate ./abis ./artifacts/*.json",
    "build:forge": "echo '> Building contracts...'; cd lib/contracts && forge install && forge build --sizes",
    "build:sdk": "echo '> Compiling SDK...'; yarn run build:esm && yarn run build:cjs",
    "build:esm": "echo '> Compiling ESM SDK Module...'; tsc -p tsconfig-sdk.json",
    "build:cjs": "echo '> Compiling CJS SDK Module...'; tsc -p tsconfig-cjs.json",
    "setup": "cd lib/contracts && forge build",
    "test:mocha": "nyc ts-mocha --require 'tests-mocha/globalTestHook.ts' --check-leaks --exit --timeout 60000 'tests-mocha/**/*.spec.ts'",
    "test": "hardhat test --parallel --no-compile",
    "test:bsc": "SIMULATION=true FORK_RPC_URL=${FORK_RPC_URL} FORK_CHAIN_ID=56 hardhat test --no-compile",
    "test:forge": "cd lib/contracts && forge test",
    "export": "hardhat export --export-all ./deployments.json",
    "export:all": "npm run export:bsc && npm run export:polygon && npm run export:neondevnet && npm run export:chapel && npm run export:arbitrum",
    "export:bsc": "hardhat export --network bsc --export ../chains/deployments/bsc.json",
    "export:polygon": "hardhat export --network polygon --export ../chains/deployments/polygon.json",
    "export:ethereum": "hardhat export --network ethereum --export ../chains/deployments/ethereum.json",
    "export:arbitrum": "hardhat export --network arbitrum --export ../chains/deployments/arbitrum.json",
    "export:neondevnet": "hardhat export --network neondevnet --export ../chains/deployments/neondevnet.json",
    "export:lineagoerli": "hardhat export --network lineagoerli --export ../chains/deployments/lineagoerli.json",
    "export:chapel": "hardhat export --network chapel --export ../chains/deployments/chapel.json",
    "export:ganache": "hardhat export --network localhost --export ../chains/deployments/ganache.json",
    "prettier:ts": "prettier --write 'chainDeploy/**/*.ts' 'src/**/*.ts' 'deploy/**/*.ts' 'tasks/**/*.ts' 'tests/**/*.ts'",
    "lint": "eslint . -c .eslintrc.json --ext ts,json",
    "lint-fix": "eslint . -c .eslintrc.json --fix --ext ts,json",
    "deploy:chapel": "hardhat --network chapel deploy --tags prod --no-compile",
    "deploy:bsc": "hardhat --network bsc deploy --tags prod --no-compile",
    "deploy:polygon": "hardhat --network polygon deploy --tags prod --no-compile",
    "deploy:ethereum": "hardhat --network ethereum deploy --tags prod --no-compile",
    "deploy:arbitrum": "hardhat --network arbitrum deploy --tags prod --no-compile",
    "deploy:neondevnet": "hardhat --network neondevnet deploy --tags prod --no-compile",
    "deploy:lineagoerli": "hardhat --network lineagoerli deploy --tags prod --no-compile",
    "deploy:localhost": "hardhat --network localhost deploy --tags prod --no-compile",
    "deploy:localeth": "hardhat --network localeth deploy --tags prod --no-compile",
    "forge": "forge",
    "hardhat": "hardhat"
  },
  "keywords": [],
  "author": "Ionic Protocol",
  "license": "ISC",
  "devDependencies": {
    "@abimate/core": "^0.2.1",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@tenderly/hardhat-tenderly": "^1.7.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/bad-words": "^3.0.1",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7",
    "@types/chai-subset": "^1",
    "@types/eslint": "^8.37.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/sinon-chai": "^3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "hardhat": "^2.14.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-deploy": "^0.11.26",
    "lint-staged": "13.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.8.7",
    "prettier-plugin-solidity": "1.1.3",
    "sinon": "^15.0.3",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@ionicprotocol/chains": "workspace:*",
    "@ionicprotocol/types": "workspace:*",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "bad-words": "^3.0.4",
    "bufferutil": "^4.0.7",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "utf-8-validate": "^6.0.3"
  }
}
