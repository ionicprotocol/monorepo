{
  "overrides": [
    {
      "files": ["*.sol", "*.ts", "*.tsx", ".js"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "explicitTypes": "always",
        "trailingComma": "none"
      }
    }
  ]
}
